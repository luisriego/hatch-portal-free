            <script>

                function SearchBoxGo() {
                    if(document.getElementById('q').classList.contains('active')){
                        //was active so text box is going to collapse
                        if((document.getElementById('q').value.trim()+'')!=''){
                            //prevent from collapsing if not empty
                            setTimeout("document.getElementById('q').classList.add('active')",1);
                            //do not move focus into text box
                            //overlay the page by a semitransparent layer with a red spinner
                            //OverlaySpinner();
                            //initiate search
                            BingSearchGo();
                            //change search icon to spinning refresh icon
                            document.getElementById('searchboxicon').classList.remove('glyphicon-search');
                            document.getElementById('searchboxicon').classList.add('glyphicon-refresh');
                            document.getElementById('searchboxicon').classList.add('gly-spin');
                            //prevent from collapsing if not empty
                            //document.getElementById('q').classList.add('active');
                        }
                        else{
                            //let it collapse if empty
                            //make sure the icon is search icon and not spinning refresh icon
                            document.getElementById('searchboxicon').classList.remove('gly-spin');
                            document.getElementById('searchboxicon').classList.remove('glyphicon-refresh');
                            document.getElementById('searchboxicon').classList.add('glyphicon-search');
                            //do not move focus into text box
                        }
                    }
                    else{
                        //was not active so text box is going to expand
                        //make sure the icon is search icon and not spinning refresh icon
                        document.getElementById('searchboxicon').classList.remove('gly-spin');
                        document.getElementById('searchboxicon').classList.remove('glyphicon-refresh');
                        document.getElementById('searchboxicon').classList.add('glyphicon-search');
                        //move focus into text box after it expands
                        setTimeout("SetFocus('q')",200);
                    }
                }

                function SetFocus(id){
                    //move focus into text box
                    document.getElementById(id).focus();
                }

                function OverlaySpinner(){
                    var overlay = document.createElement("div");
                    overlay.setAttribute("id","overlay");
                    overlay.setAttribute("class", "overlay");
                    document.body.appendChild(overlay);
                }

                function SwitchLanguage(control) {

                    var lang = control.value;
                    var langExp = 'en|es-CL|fr-CA';

                    var href = location.pathname;
                    var alllang = langExp.split('|');
                    $(alllang).each(function(index, element) {
                        if(href.endsWith("/"+element))
                        {
                            href = href + "/";
                        }
                    });
                    href = href.replace(/\/(en|es-CL|fr-CA)\//i, "/");
                    // href = href.replace(/(en|es-CL|fr-CA)/i, "");
                    if (lang == $('#languageToggle').attr("defualtlanguage"))
                    {
                        location.href = location.origin + href;
                    }
                    else
                    {
                        location.href = location.origin + "/" + lang + href;

                    }
                    /*  $.ajax({
                          url: "/api/sitecore/HatchSearch/LangSelector",
                          methode: "POST",
                          dataType: "application/json",
                          data: {
                              language: lang
                          },
                          complete: function () {

                              var href = location.origin + "/" + lang + location.pathname.replace(/\/(en|es-CL|fr-CA)\//i, "");


                                      location.href = location.origin + "/" + lang + location.pathname.replace(/\/(en|es-CL|fr-CA)\//i, "/");


                          }
                      })*/
                }
                function BingSearch(e) {
                    if (e.keyCode == 13) {
                        SearchBoxGo();
                    }
                    else
                    {
                        var queryparam  = $('#q').val()
                        var param = { q: $('#q').val() };
                        var urlStr = (window.location.href.substr(window.location.href.indexOf('/'))).substr(2);
                        urlStr = urlStr.substr(0,urlStr.indexOf('/'));
                        if(queryparam.length > 1)
                        {
                            $.ajax({
                                url: "https://"+urlStr+"/HatchSearch/AutoSearch",
                                data: JSON.stringify(param),
                                dataType: "json",
                                type: "POST",
                                contentType: "application/json; charset=utf-8",
                                dataFilter: function (data) { return data; },
                                success: function (data) {
                                    if(data.length > 0)
                                    {
                                        if($("#searchsugg") !== undefined)
                                        {
                                            $("#searchsugg").remove();

                                        }
                                        // console.log(JSON.stringify(data));


                                        htmlstring = '<div id="searchsugg" Class="searchsuggestion"><ul class = "searchsuggestionlist">'
                                        $(data).each(function(index, element) {
                                            if(element.Title.length > 50)
                                            {
                                                htmlstring += '<li><a href = ' +element.Url.replace(" ", "-")+ '><div  class="ssli" width="100%">'+element.Title.substring(0,50)+'...</div></a></li>';
                                            }
                                            else
                                            {
                                                htmlstring += '<li><a href = ' +element.Url.replace(" ", "-")+ '><div class="ssli" width="100%">'+element.Title+'</div></a></li>';
                                            }

                                        });
                                        htmlstring += '</ul></div>'

                                        $("#search-box-typeahead").append(htmlstring);
                                        $("#searchsugg")[0].style.display = 'block';

                                    }
                                    else
                                    {
                                        $("#searchsugg")[0].style.display = 'none'
                                    }
                                }
                                ,
                                error: function (errorThrown) {

                                    alert("Error")

                                }



                            });
                        }
                        else
                        {
                            $("#searchsugg")[0].style.display = 'none'
                        }
                    }
                }

                function BingSearchGo() {
                    var keyword = $("#q").val();
                    window.location = '/Search-Results' + '?q=' + keyword;
                }

                $(document).mouseup(function(e)
                {

                    $("#searchsugg")[0].style.display = 'none'
                });
                function BingSearchCheckbox(checkbox) {
                    if (checkbox.checked == true)
                    {

                        var keyword1 = GetParameterValues('q');
                        var keyword2 = checkbox.value;
                        window.location = '/Search-Results' + '?q=' + keyword1 + '&p='+keyword2;
                    }
                    else
                    {
                        var keyword1 = GetParameterValues('q');

                        window.location = '/Search-Results' + '?q=' + keyword1;
                    }

                }

                function GetParameterValues(param) {
                    var url = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                    for (var i = 0; i < url.length; i++) {
                        var urlparam = url[i].split('=');
                        if (urlparam[0] == param) {
                            return urlparam[1];
                        }
                    }
                }
            </script>
